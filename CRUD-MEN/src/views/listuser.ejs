<div class="container pt-3">
    <h1>List of User</h1>
    <table class="table-hover table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>Sr.no</th>
                <th>Content</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% let c = 1 %>
            <% data.forEach(element => { %>
                <tr>
                <td><%= c %> </td>
                <td><%= element.fname %></td>
                <td>
                    <button class="btn btn-outline-primary"><a style="color: initial;text-decoration: none;" href="/user/edit?id=<%= element._id %> ">Edit</a></button>
                    <button class="btn btn-outline-danger"><a style="color: initial;text-decoration: none;" href="/user/remove?id=<%= element._id %>">Delete</a></button>
                </td>
            </tr>
            <%  c = c + 1 ;}); %> 
        </tbody>
    </table>
</div>
   